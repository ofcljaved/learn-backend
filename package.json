{
  "name": "learn-backend",
  "version": "1.0.0",
  "description": "A series to enhance backend with chai aur code",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/",
      "node_modules/"
    ],
    "watch": [
      "src"
    ],
    "ext": ".ts,.js",
    "exec": "ts-node ./src/index.ts"
  },
  "keywords": [
    "backend",
    "typescript"
  ],
  "author": "ofcljaved",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
